marks = int(input("Say your grade: "))

if marks <= 0:
    print("Invalid Score! 0-100 only")
if marks >= 101:
    print("Invalid Score! 0-100 only")
if marks >= 90:
    print("GRADE: A")
elif marks >= 80:
    print("GRADE: B")
elif marks >= 70:
    print("GRADE: C")
elif marks >= 60:
    print("GRADE: D")
else:
    print("GRADE: F")
